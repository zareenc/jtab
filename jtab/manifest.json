{
    "manifest_version": 2,
    "name": "JTab",
    "version": "0.0.0.1",
    "description": "Making tabs great again - almost as great as Japacurry",
    "icons": {},
    "browser_action": {
        "default_title": "jtab"
    },
    "author": "Distsys-Japa",
    "background": {
        "persistent": false,
        "scripts": [
            "js/storage.js",
            "js/helpers.js",
            "js/tabmanager.js",
            "background.js"
        ]
    },
    "commands": {},
    "options_page": "options.html",
    "options_ui": {
        "chrome_style": true,
        "page": "options.html"
    },
    "permissions": [
        "tabs"
    ]
}
